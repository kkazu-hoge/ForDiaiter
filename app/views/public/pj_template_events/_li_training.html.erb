                        <%= form_with url: new_wizard3_projects_path, method: :get do |f| %>
                          <div class="row">
                            <div class="col-8">
                              <ul class="sample1 training_select_scroll">
                                <!-- //eachでリストを動的に追加する -->
                                <li>
                                  <div class="row">
                                    <div class="col-6">
                                      <%= training_name %>（10分:<%= calorie_per_ten_min %>kcal）
                                    </div>
                                    <div class="col-3">
                                      <%= f.text_field :name, class: "form-control form-control-sm", placeholder: "時間：分" %>
                                    </div>
                                    <div class="col-2">
                                      <btn>削除</btn>
                                    </div>
                                  </div>
                                </li>
                              </ul>
                            </div>
                            <div class="col-4">
                              <div class="row">
                                <div class='col-12'>
                                  <%=link_to "トレーニング追加", new_pj_template_event_path, class: 'btn col-8 mx-auto btn-danger font-weight-bold btn-block', remote: true %>
                                </div>
                              </div>
                              <div class='row login_input_field login_button'>
                                <div class='col-12'>
                                  <div class="actions">
                                    <%= f.submit "PJ作成完了",class: 'btn col-8 mx-auto btn-outline-secondary font-weight-bold btn-block' %>
                                  </div>
                                </div>
                              </div>
                            </div>

                          </div>
                        <% end %>


